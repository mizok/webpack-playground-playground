<!DOCTYPE html>
<html lang="en">

<%- include('./template/head.ejs',{title:'Webpack Playground Template'})%>

<body>
  <div class="playground" id="playground">
    <div class="playground__inner">
      <div class="playground__sidebar sidebar">
        <h1 class="sidebar__title">
          Webpack Playground Template
        </h1>
        <div class="sidebar__menu">
          <%- include('./template/menu.ejs') %>
        </div>
        <div class="sidebar__footer">
          <div class="author">
            <div class="author__name">Mizok.H</div>
            <div class="author__links">
              <a href="https://github.com/mizok" class="author__link">
                <img src="~@img/github.svg" alt="">
              </a>
              <a href="https://twitter.com/MizokStudio" class="author__link">
                <img src="~@img/twitter.svg" alt="">
              </a>
            </div>
          </div>
        </div>
      </div>
      <div class="playground__body">
        <button class="playground__toggler hamburger hamburger--collapse" id="playground-toggler" type="button">
          <span class="hamburger-box">
            <span class="hamburger-inner"></span>
          </span>
        </button>
        <div class="playground__loading" id="loading">
          <%- include('./template/loading-icon.ejs') %>
        </div>
        <iframe src="examples/demo.html" frameborder="0" class="playground__content" id="playground-content"></iframe>
      </div>
    </div>
  </div>
</body>

</html>